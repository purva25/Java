<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>GradedActivity</display-name>
  
  <servlet>
  <servlet-name>Servlet</servlet-name>
  <servlet-class>Servlets.SimpleServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Servlet</servlet-name>
  <url-pattern>/demo</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Servlet2</servlet-name>
  <servlet-class>Servlets.Generic_Servlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Servlet2</servlet-name>
  <url-pattern>/gdemo</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Servlet3</servlet-name>
  <servlet-class>Servlets.Http_Servlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Servlet3</servlet-name>
  <url-pattern>/hdemo</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>